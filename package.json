{
  "name": "TMN-WEB",
  "version": "1.0.0",
  "description": "This is the Web app for The-Music-Network",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run build:client && webpack-dev-server --config ./build/webpack.config.js --open --watch --hot --progress",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js",
    "test": "mocha",
    "test:show": "cross-env NODE_ENV=test:show mocha",
    "test:slow": "cross-env NODE_ENV=test:slow mocha",
    "start": "npm run build && npm run start:server",
    "start:server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theRealScoobaSteve/TMN-WEB.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theRealScoobaSteve/TMN-WEB/issues"
  },
  "homepage": "https://github.com/theRealScoobaSteve/TMN-WEB#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-prettier": "^2.6.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.14",
    "kermit-pptr": "^0.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha": "^5.2.0",
    "mocha-prepare": "^0.1.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "puppeteer": "^1.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "vue-server-renderer": "^2.5.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  }
}
